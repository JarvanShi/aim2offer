x = int(input())
if x>1:
    left = 1
    right = x
else:
    left = 0
    right = x
mid = x/2

while abs(pow(mid, 2)-x)>=0.001:
    if x == 0 or x == 1:
        print(x)
        break
    elif mid*mid > x:
        right = mid
    elif mid*mid <= x:
        left = mid
    mid = (left + right) /2
    print(left, mid, right)
print('{:.4f}'.format(mid))
