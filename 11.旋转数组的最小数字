#! /usr/bin/env python3
class Solution:
    def minNumberInRotateArray(self, rotateArray):
        n = len(rotateArray)
        if rotateArray == []:
            return 0
        elif n == 1:
            return rotateArray
        else:
            for i in range(n-1):
                if rotateArray[i] > rotateArray[i+1]
                return rotateArray[i+1]
